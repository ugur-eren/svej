{
  "name": "svej-backend",
  "description": "Created with project-manager CLI",
  "private": true,
  "version": "1.0.0",
  "license": "GPL-2.0",
  "scripts": {
    "start": "ts-node ./index.ts",
    "watch": "nodemon ./index.ts",
    "test": "jest ./tests --passWithNoTests",
    "format:check": "prettier -c \"src/**/*.ts\"",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint:check": "eslint \"src/**/*.ts\"",
    "lint": "yarn lint:check --fix",
    "ts:check": "tsc --noEmit",
    "check": "yarn format && yarn format:check && yarn lint:check && yarn ts:check"
  },
  "dependencies": {
    "dotenv": "^16.1.3",
    "backend": "*"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.24",
    "eslint": "^8.5.0",
    "jest": "^27.5.1",
    "nodemon": "^2.0.15",
    "rimraf": "^3.0.2",
    "ts-node": "^10.4.0",
    "typescript": "~4.9.4"
  }
}
